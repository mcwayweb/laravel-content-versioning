{
	"name" : "mcwayweb/laravel-content-versioning",
	"description" : "Add content versioning behavior to your Laravel app.",
	"keywords" : [
		"mcwayweb",
		"laravel-content-versioning"
	],
	"homepage" : "https://github.com/mcwayweb/laravel-content-versioning",
	"license" : "MIT",
	"authors" : [{
			"name" : "Robert McWay",
			"email" : "robert@mcwayweb.tech",
			"homepage" : "https://www.mcwayweb.tech",
			"role" : "Software Engineer, AI Data Scientist, Self Actualized Truth Seeking Psychedelic Yogi on an existential mission to decode reality..."
		}
	],
	"require" : {
		"php" : "^7.4",
		"laravel/framework" : "^8.0"
	},
	"require-dev" : {
		"friendsofphp/php-cs-fixer" : "^2.16",
		"orchestra/testbench" : "^6.0",
		"phpunit/phpunit" : "^9.3"
	},
	"autoload" : {
		"psr-4" : {
			"Spatie\\Tags\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Spatie\\Tags\\Test\\" : "tests"
		}
	},
	"scripts" : {
		"test" : "vendor/bin/phpunit",
		"format" : "vendor/bin/php-cs-fixer fix --allow-risky=yes"
	},
	"config" : {
		"sort-packages" : true
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Mcwayweb\\ContentVersioning\\ContentVersioningServiceProvider"
			]
		}
	},
	"minimum-stability" : "dev",
	"prefer-stable" : true
}